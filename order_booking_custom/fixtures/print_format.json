  "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"Custom HTML\", \"options\": \"<!--<div class=\\\"print-heading\\\">\\t\\t\\t\\t<h2><div>Order Booking Form</div><br><small class=\\\"sub-heading\\\">{{ doc.name }}</small>\\t\\t\\t\\t</h2></div>-->\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"_custom_html\", \"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldtype\": \"HTML\", \"options\": \"<b>Order Booking: </b>&emsp;&emsp;&emsp;&emsp;&nbsp;{{ doc.name }}\"}, {\"fieldname\": \"date\", \"print_hide\": 0, \"label\": \"Date\"}, {\"fieldname\": \"customer_name\", \"print_hide\": 0, \"label\": \"Customer Name\"}, {\"fieldname\": \"_custom_html\", \"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldtype\": \"HTML\", \"options\": \"<b>GSTIN:</b>&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&ensp;{{frappe.db.get_value(\\\"Customer\\\",doc.customer_name,\\\"gstin\\\")}}\"}, {\"fieldname\": \"area_code\", \"print_hide\": 0, \"label\": \"Area Code\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldtype\": \"Section Break\", \"label\": \"Order Details\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"order_booking_details\", \"print_hide\": 0, \"label\": \"Order Booking Details\", \"visible_columns\": [{\"fieldname\": \"tyre_size\", \"print_width\": \"\", \"print_hide\": 0}, {\"fieldname\": \"tyre_brand\", \"print_width\": \"\", \"print_hide\": 0}, {\"fieldname\": \"tyre_make\", \"print_width\": \"\", \"print_hide\": 0}, {\"fieldname\": \"tyre_serial_number\", \"print_width\": \"\", \"print_hide\": 0}, {\"fieldname\": \"qty\", \"print_width\": \"\", \"print_hide\": 0}, {\"fieldname\": \"remark\", \"print_width\": \"\", \"print_hide\": 0}]}]",
  "html": "{% set customer_name = \"Ankush Enterprises\" %}\n{% set address_name = frappe.db.get_value(\"Dynamic Link\", {\"link_doctype\": \"Customer\", \"link_name\": customer_name}, \"parent\") %}\n{% set address_line1 = frappe.db.get_value(\"Address\", {\"name\": address_name}, \"address_line1\") %}\n{% set address_line2 = frappe.db.get_value(\"Address\", {\"name\": address_name}, \"address_line2\") %}\n{% set city = frappe.db.get_value(\"Address\", {\"name\": address_name}, \"city\") %}\n{% set state = frappe.db.get_value(\"Address\", {\"name\": address_name}, \"state\") %}\n{% set country = frappe.db.get_value(\"Address\", {\"name\": address_name}, \"country\") %}\n{% set pincode = frappe.db.get_value(\"Address\", {\"name\": address_name}, \"pincode\") %}\n{% set gstin = frappe.db.get_value(\"Address\", {\"name\": address_name}, \"gstin\") %}\n<!--<p>Customer Name: {{ customer_name }}</p>-->\n<!--<p>Address: {{ address }}</p>-->\n\n\n\n<div class=\"letter-head\">\n    <img src=\"/files/ECCO TYRE LOGO.jpg\" alt=\"Ecco Tyres Logo\" />\n</div>\n\n<div class=\"letter-head-second\">\n    <img src=\"/files/WhatsApp Image 2025-01-25 at 12.34.30 PM (1).jpeg\" alt=\"Ecco Tyres Logo\" />\n</div>\n\n\n\n\n<table style=\"font-size: 14px; width: 100%; margin-top: 0px; border-collapse: collapse;\">\n    <tr>\n        \n        <td style=\"text-align: left; padding: 8px;\">\n            <b>Customer Name:</b> {{ doc.customer_name }}\n        </td>\n        <td style=\"padding: 8px; font-weight:8px; text-align:right\">\n           <b>Order Booking Number:</b>  {{ doc.name }}\n        </td>\n        \n        \n    </tr>\n    <tr>\n        <td style=\"padding: 8px;\">\n            <b>Branch:</b> {{ doc.custom_branch }}\n        </td>\n        <td style=\"text-align: right; padding: 8px;\">\n            <b>Area Code:</b> {{ doc.area_code }}\n        </td>\n        \n        \n        \n    </tr>\n    <tr>\n        <td style=\"padding: 8px;\">\n            <b>Address:</b>\n            {{ address_line1 }},\n            {{ address_line2 }}<br>\n            {{ city }}, {{ state}},{{ country }},{{ pincode }}\n        </td>\n        <td style=\"text-align: right; padding: 8px;\">\n            <b>Date:</b> {{ frappe.utils.formatdate(frappe.utils.nowdate(), \"d MMMM yyyy\") }}\n        </td>\n        \n    </tr>\n    <tr>\n        <td style=\"padding: 8px;\">\n            <b>GSTIN / UIN:</b>\n            {{ gstin }}\n        </td>\n    </tr>\n</table>\n<div>\n    <center>\n        <h3 style=\"font-size: 18px; margin-top: 100px;\">\n            <u>Order Booking Form</u>\n        </h3>\n    </center>\n</div>\n\n<table class=\"table table-bordered table-condensed\" style=\"width: 100%; font-size: 14px; border: 1px solid #ccc; border-collapse: collapse;\">\n    <thead>\n        <tr style=\"background-color: #f9f9f9; font-weight: bold; text-align: center; border-bottom: 1px solid #ccc;\">\n            <th style=\"padding: 8px; border: 1px solid #ccc; width: 10%;\">SI No.</th>\n            <th style=\"padding: 8px; border: 1px solid #ccc; width: 15%;\">Tyre Size</th>\n            <th style=\"padding: 8px; border: 1px solid #ccc; width: 15%;\">Tyre Brand</th>\n            <th style=\"padding: 8px; border: 1px solid #ccc; width: 15%;\">Tyre Make</th>\n            <th style=\"padding: 8px; border: 1px solid #ccc; width: 15%;\">Tyre Serial Number</th>\n            <th style=\"padding: 8px; border: 1px solid #ccc; width: 15%;\">Pattern</th>\n            <th style=\"padding: 8px; border: 1px solid #ccc; width: 15%;\">Remark</th>\n        </tr>\n    </thead>\n    <tbody>\n        {% for item in doc.order_booking_details %}\n        <tr style=\"text-align: center; border-bottom: 1px solid #ccc;\">\n            <td style=\"padding: 8px; border: 1px solid #ccc;\">{{ loop.index }}</td>\n            <td style=\"padding: 8px; border: 1px solid #ccc;\">{{ item.tyre_size }}</td>\n            <td style=\"padding: 8px; border: 1px solid #ccc;\">{{ item.tyre_brand }}</td>\n            <td style=\"padding: 8px; border: 1px solid #ccc;\">{{ item.tyre_make }}</td>\n            <td style=\"padding: 8px; border: 1px solid #ccc;\">{{ item.tyre_serial_number }}</td>\n            <td style=\"padding: 8px; border: 1px solid #ccc;\">{{ item.qty }}</td>\n            <td style=\"padding: 8px; border: 1px solid #ccc;\">{{ item.remark }}</td>\n        </tr>\n        {% endfor %}\n    </tbody>\n</table>\n\n<div style=\"margin-top: 20px; font-size: 14px;\">\n    <p>1. Inspection by our representative is preliminary, and inspection at our factory is the only<br>   inspection binding upon us.</p>\n    <p>2. Patches/Repair charges are extra at actuals.</p>\n    <p>3. Please produce this form while obtaining delivery of retreaded/rejected tyres.</p>\n    <p>4. Tyres not collected within one month will be disposed of without any notice.</p>\n</div>\n\n<!--<div style=\"text-align: right; font-size: 14px; margin-top: 30px;\">-->\n<!--    For Eccotyres Retreading India Private Limited-->\n<!--</div>-->\n<!--<br>-->\n<!--<br>-->\n<!--<br>-->\n<!--<br>-->\n<!--<table style=\"width: 100%; margin-top: 20px; font-size: 14px; border-collapse: collapse;\">-->\n<!--    <tr>-->\n<!--        <td style=\"padding: 8px; text-align: left;\">-->\n<!--            <b>Receiver's Signature</b>-->\n<!--        </td>-->\n<!--        <td style=\"padding: 8px; text-align: right;\">-->\n<!--            <b>Authorised Signatory</b>-->\n<!--        </td>-->\n<!--    </tr>-->\n</table>\n\n\n   <div id=\"footer-html\" class=\"visible-pdf\">\n<div class=\"print-format-footer\">\n        {{ footer }}\n    </div>\n    \n</div>\n",
