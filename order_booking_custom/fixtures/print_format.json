  "html": "<!-- Table with Colspan and Rowspan in Frappe Jinja Print Format -->\r\n<table class=\"table table-bordered\">\r\n    <thead>\r\n        <tr>\r\n            <td colspan=\"4\">\r\n                <figure>\r\n\r\n                <img style=\"width: 74% !important;\"  src=\"http://bwipjs-api.metafloor.com/?bcid=pdf417&text={{ doc.serial_no }} \" alt=\"barcode\" align=\"middle\" hspace=\"20\">\r\n                <figcaption align=\"middle\">{{ doc.serial_no }}</figcaption>\r\n                \r\n                <figure>\r\n    \r\n            </td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"6\" style=\"font-size:15px;text-align:center\">Retread Process Work Order</td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"3\">CUSTOMER ORDER:<br>{{doc.sales_order}}</td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"5\" style=\"text-align:center;\">Work Order No: <br>{{doc.name}}</td>\r\n            <td colspan=\"2\">Work Order Date <br> {{ doc.creation }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"3\">Customer Code:<br>{{frappe.db.get_value(\"Order Booking Form\",{\"name\":doc.custom_order_booking},\"customer_name\")}}</td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"7\" style=\"text-align:center\">Requested Tread Pattern: T123456</td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"10\">CUSTOMER NAME:{{frappe.db.get_value(\"Order Booking Form\",{\"name\":doc.custom_order_booking},\"customer_name\")}} </td>  <!-- Merge 6 columns for header -->\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"3\">TYRE DETAILS :</td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"7\" style=\"text-align:center\">Requested Delivery Date: {{frappe.db.get_value(\"Order Booking Form\",{\"name\":doc.custom_order_booking},\"delivery_date\")}}</td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"3\">Tyre Make {{frappe.db.get_value(\"Order Booking Details\",{\"parent\":doc.custom_order_booking,\"item_code\":doc.production_item},\"tyre_make\")}} </td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"3\">Tyre Model {{frappe.db.get_value(\"Order Booking Details\",{\"parent\":doc.custom_order_booking,\"item_code\":doc.production_item},\"tyre_brand\")}}</td>\r\n            <td colspan=\"4\">Tyre Serial No {{frappe.db.get_value(\"Order Booking Details\",{\"parent\":doc.custom_order_booking,\"item_code\":doc.production_item},\"tyre_serial_number\")}}</td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"3\">Tyre Size {{frappe.db.get_value(\"Order Booking Details\",{\"parent\":doc.custom_order_booking,\"item_code\":doc.production_item},\"tyre_size\")}}</td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"3\">Tyre Received Date </td>\r\n            <td colspan=\"4\">Prepared By </td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"9\" style=\"text-align:center\">To Be Filled By Operator</td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"1\">Operator Name</td>\r\n        </tr>\r\n        <tr>\r\n            <td rowspan=\"2\">Initial Inspection (1)</td> <!-- Row span for main heading -->\r\n            <td rowspan=\"2\">Date</td> <!-- Row span for Date -->\r\n            <td colspan=\"1\">Casing Grading</td> <!-- Merging columns -->\r\n            <td colspan=\"1\">Process Type: Retreading</td>\r\n            <td colspan=\"1\">Additional Damage : N</td> <!-- Row span for Tread -->\r\n            <td colspan=\"1\">Casing Status: 1</td> <!-- Row span for BVC -->\r\n            <!--<th rowspan=\"2\">Trade Length</th>-->\r\n            <td colspan=\"2\" rowspan=\"2\" style=\"width:50px;\">Start/End time</td> <!-- Row span for Start time -->\r\n            <!--<td colspan=\"2\"></th>-->\r\n        </tr>\r\n        <tr>\r\n            <td colspan= \"2\">No Of Injuries</td> <!-- First sub-column under Rope rubber -->\r\n            <td colspan= \"2\">Pattern onfirmation</td> <!-- Second sub-column under Rope rubber -->\r\n            <td colspan= \"2\"></td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"2\">Buffing (2)</td>\r\n            <td colspan=\"1\" v>Date</td>\r\n            <td colspan=\"1\">Buff Radius</td>\r\n            <td colspan=\"1\">Buff Crown Width (MM)</td>\r\n            <td colspan=\"1\">Buffed Circumference (MM)</td>\r\n            <td colspan=\"2\">Buff Circumference (MM)</td>\r\n            <td colspan=\"1\">Blades</td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <td rowspan=\"8\">Buffing (2)</td>\r\n            <td colspan=\"2\" style=\"height:50px\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"2\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <!--<td colspan=\"1\"></td>-->\r\n            \r\n        </tr>\r\n        <tr>\r\n            <!--<td colspan=\"2\">Buffing (2)</td>-->\r\n            <td colspan=\"2\" v>Repair Numbers:</td>\r\n            <td colspan=\"1\">Injury Location</td>\r\n            <td colspan=\"1\">Diameter of Injury (mm)</td>\r\n            <td colspan=\"1\">Width (mm)</td>\r\n            <td colspan=\"2\">Length (mm)</td>\r\n            <td colspan=\"1\">Patch Size</td>\r\n            <td colspan=\"1\"></td>\r\n            <!--<td colspan=\"1\"></td>-->\r\n            \r\n        </tr>\r\n        <tr>\r\n            <!--<td colspan=\"2\">Buffing (2)</td>-->\r\n            <td colspan=\"2\" style=\"height:50px\" >Repair 1</td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"2\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <!--<td colspan=\"2\">Buffing (2)</td>-->\r\n            <td colspan=\"2\" style=\"height:50px\" >Repair 2</td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"2\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <!--<td colspan=\"2\">Buffing (2)</td>-->\r\n            <td colspan=\"2\" style=\"height:50px\" >Repair 3</td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"2\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <!--<td colspan=\"2\">Buffing (2)</td>-->\r\n            <td colspan=\"2\" style=\"height:50px\" >Repair 4</td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"2\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <!--<td colspan=\"2\">Buffing (2)</td>-->\r\n            <td colspan=\"2\" style=\"height:50px\" >Repair 5</td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"2\"></td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <!--<td colspan=\"2\">Buffing (2)</td>-->\r\n            <td colspan=\"2\" v>Date</td>\r\n            <td colspan=\"5\">Rejection Reason: </td>\r\n            <td colspan=\"1\">Start/ End Time:</td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <td rowspan=\"2\">Tyre Preparation (4)</td> <!-- Row span for main heading -->\r\n            <td rowspan=\"2\">Date</td> <!-- Row span for Date -->\r\n            <td colspan=\"1\">Rope rubber (B. No)</td> <!-- Merging columns -->\r\n            <td colspan=\"1\">B.G (B. No)</td>\r\n            <td colspan=\"1\">Tread (B. No)</td> <!-- Row span for Tread -->\r\n            <td colspan=\"1\">BVC (B. No)</td> <!-- Row span for BVC -->\r\n            <!--<th rowspan=\"2\">Trade Length</th>-->\r\n            <td colspan=\"2\" rowspan=\"2\" style=\"width:50px;\">Start/End time</td> <!-- Row span for Start time -->\r\n            <!--<td colspan=\"2\"></th>-->\r\n        </tr>\r\n        <tr>\r\n            <td colspan= \"2\">Trade Pattern</td> <!-- First sub-column under Rope rubber -->\r\n            <td colspan= \"2\">Tread Length</td> <!-- Second sub-column under Rope rubber -->\r\n            <td colspan= \"2\"></td>\r\n        </tr>\r\n        <tr>\r\n            <td rowspan=\"1\">Building(5)</td>\r\n            <td colspan=\"1\">Date</td>\r\n            <td colspan=\"3\">B.G (B. No)<br></td>\r\n            <td colspan=\"3\">B. G (Width)</td>\r\n            <td colspan=\"1\" style=\"width: 50px;\">Time:</td>  <!-- Set custom width -->\r\n            <td colspan=\"1\"></td>\r\n        </tr>\r\n        <tr>\r\n            <td rowspan=\"2\">Enveloping (6)</td> <!-- Row span for two rows -->\r\n            <td rowspan=\"2\">Date</td> <!-- Row span for Date -->\r\n            <td rowspan=\"2\">Type of Sealing</td> <!-- Row span for Sealing -->\r\n            <td colspan=\"2\">O. Env. (Size & S. No)</td> <!-- Merge two columns -->\r\n            <td colspan=\"2\">I. Env. (Size & S. No)</td>\r\n            <td rowspan=\"2\" style=\"width:50px;\">Start time</td> <!-- Row span for Start time -->\r\n            <td colspan=\"2\"></th>\r\n        </tr>\r\n        <tr>\r\n            <td>Size</td> <!-- First column for O. Env -->\r\n            <td>S. No</td> <!-- Second column for O. Env -->\r\n            <td>Size</td> <!-- First column for O. Env -->\r\n            <td>S. No</td>\r\n        </tr>\r\n        <tr>\r\n            <td rowspan=\"1\">Curing(7)</td>\r\n            <td colspan=\"1\">Date</td>\r\n            <td colspan=\"3\">Chamber<br></td>\r\n            <td colspan=\"3\">Remarks</td>\r\n            <td colspan=\"1\" style=\"width: 50px;\">Time:</td>  <!-- Set custom width -->\r\n            <td colspan=\"1\"></td>\r\n        </tr>\r\n\r\n        <tr >\r\n            <td rowspan=\"2\">Final Inspection (8)</td>\r\n            <td colspan=\"1\">Date</td>\r\n            <td colspan=\"3\">Accepted / Rejected / Rework</td>\r\n            <td colspan=\"2\">Reason for Rejected / Rework</td>\r\n            <td colspan=\"1\" style=\"display: table;\"></td>\r\n            <td colspan=\"1\"></td>\r\n        </tr>\r\n        <tr>\r\n            <!--<td rowspan=\"2\">Final Inspection (8)</td>-->\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"3\">Reason for Rework</td>\r\n            <td colspan=\"3\">Rework Stage Number</td>\r\n            <td colspan=\"1\"></td>\r\n            <td colspan=\"1\"></td>\r\n            \r\n        </tr>\r\n        \r\n        <tr>\r\n            <td colspan=\"1\">REMARKS</td>  <!-- Merge 6 columns for header -->\r\n            <td colspan=\"9\">OK</td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"10\" style=\"text-align: right;\">\r\n                <figure>\r\n\r\n                <img style=\"float: right;width: fit-content !important;margin: 0;\"   src=\"http://bwipjs-api.metafloor.com/?bcid=pdf417&text={{ doc.serial_no }} \" alt=\"barcode\" align=\"middle\" hspace=\"10\">\r\n                <figcaption align=\"middle\" style=\"float:right;\">{{ doc.serial_no }}</figcaption>\r\n                \r\n                <figure></td>  <!-- Merge 6 columns for header -->\r\n            </tr>\r\n        {% set company_address_name = frappe.db.get_value('Dynamic Link', {'link_name': doc.company, 'link_doctype': 'Company', 'parenttype': 'Address'}, 'parent') %}\r\n      