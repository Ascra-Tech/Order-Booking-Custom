  "html": "<style>\nh6 {\n  font-size: 10px;\n}\n\nh5 {\n  font-size: 10px;\n}\n\np {\n  font-size: 14px;\n}\n</style>\n<table class=\"table table-bordered\">\n    <!--<tr>-->\n    <!--    <td colspan=\"5\" rowspan=\"2\">M/s.</td>-->\n    <!--    <td colspan=\"2\">ORDER NO. {{doc.name}}</td>-->\n    <!--</tr>-->\n    <!--<tr>-->\n    <!--    <td colspan=\"2\">DATE {{doc.date}}</td>-->\n    <!--</tr>-->\n    <tr>\n    {% set supplier_info = frappe.get_doc('Customer', doc.customer_name)%}\n    <!--{% set cust_info = frappe.get_doc('Customer', doc.customer_name) if doc.customer_name else None %}-->\n\n    <td>M/s. {{doc.customer_name}}</td>\n    <td>ORDER NO: {{doc.name}}</td>\n    </tr>\n    <tr>\n    <td>GSTIN: {{cust_info.gstin}}</td>\n    <td>DATE : {{doc.date}}</td>\n    </tr>\n</table>\n<table class=\"table table-condensed table-bordered\" style=\"padding:3px;margin-top:3px;\">\n\t<thead>\n\t\t<tr>\n\t\t    <th width=\"5%\">{{ _(\"Sr.No\") }}</th>\n\t\t    <th width=\"12%\">{{ _(\"Tyre Size\") }}</th>\n\t\t    <th width=\"12%\">{{ _(\"Tyre Brand\") }}</th>\n\t\t    <th width=\"12%\" class=\"text-right\">{{ _(\"Tyre Make\") }}</th>\n\t\t    <th width=\"30%\" class=\"text-right\">{{ _(\"Tyre Serial No\") }}</th>\n\t\t    <th width=\"8%\" class=\"text-right\">{{ _(\"Pattern\") }}</th>\n\t\t\t<th width=\"30%\" class=\"text-right\">{{ _(\"Remarks\") }}</th>\n\t\t</tr>\n\t</thead>\n\t<tbody>\n\t\t{%- for row in doc.order_booking_details -%}\n\t\t<tr>\n\t\t    <td>{{ loop.index }}</td>\n\t\t    <td class=\"text-left\">{{ row.tyre_size }}</td>\n\t\t    <td class=\"text-left\">{{ row.tyre_brand }}</td>\n\t\t\t<td class=\"text-left\">{{ row.tyre_make }}</td>\n\t\t\t<td class=\"text-left\">{{ row.tyre_serial_number }}</td>\n\t\t\t<td class=\"text-left\">{{ row.qty }}</td>\n\t\t\t<td class=\"text-left\">{{ row.remark }}</td>\n\t\t</tr>\n\t\t{%- endfor -%}\n\t</tbody>\n</table>\n<tr>\n\n</tr>\n<table class=\"bank-details-table\" class=\"footer-text\">\n    <tr>\n    <td colspan=\"5\">\n    <h6>\n        1.inspection by our representative is preliminary and inspection at our factory is the only inspection binding upon us.<br>\n        2.Patches / Repair charges are extra as actuals.<br>\n        3.Please produce this form while obtainig delivery of retreated / rejected tyres.<br>\n        4.TYRE NOT COLLECTED WITHIN ONE MONTH WILL BE DISPOSED WITHOUT ANY NOTICE.<br>\n    </h6>\n    </td>\n    <td colspan=\"2\">\n        <p><b>For Eccotyre Retreding India Pvt Ltd</b></p><br>\n        <p class=\"footer-text\">Authorised Signatory</p>\n    </td>\n    </tr>\n</table>\n\n<h6>\n    <tr>\n    <p style=\"text-align:center;\" class=\"footer-text\" td colspan=\"7\">Receiver's signature</p>\n    </tr>\n</h6>\n    \n    \n    ",
